{
  "extensions.ignoreRecommendations": true,
  "editor.fontFamily": "Geist Mono, Roboto Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.openEditors.visible": 1,
  "javascript.validate.enable": false,
  "material-icon-theme.showUpdateMessage": false,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.useTabs": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/build": true,
    "**/assets": true,
    "**/_build": true
  },
  "typescript.check.npmIsInstalled": false,
  "editor.snippetSuggestions": "top",
  "workbench.iconTheme": "chalice-icon-theme",
  "workbench.colorCustomizations": {
    // "tab.activeBackground": "#141414",
    // "tab.inactiveBackground": "#141414",
    // "activityBar.background": "#141414",
    // "editorGroup.emptyBackground": "#141414",
    // "sideBar.background": "#141414",
    // "titleBar.activeBackground": "#141414",
    // "editor.background": "#141414",
    // "editorGroupHeader.tabsBackground": "#141414",
    // "statusBar.background": "#1A1A1A",
    // "statusBar.noFolderBackground": "#0A0A0D",
    // "statusBar.debuggingBackground": "#511f1f"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "comment.line.double-slash",
          "constant",
          "constant.character",
          "constant.character.escape",
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.numeric.octal",
          "constant.other",
          "constant.regexp",
          "constant.rgb-value",
          "emphasis",
          "entity",
          "entity.name",
          "entity.name.class",
          "entity.name.function",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.tag",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "invalid",
          "invalid.deprecated",
          "invalid.illegal",
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.other",
          "markup",
          "markup.bold",
          "markup.changed",
          "markup.deleted",
          "markup.heading",
          "markup.inline.raw",
          "markup.inserted",
          "markup.italic",
          "markup.list",
          "markup.list.numbered",
          "markup.list.unnumbered",
          "markup.other",
          "markup.quote",
          "markup.raw",
          "markup.underline",
          "markup.underline.link",
          "meta",
          "meta.block",
          "meta.cast",
          "meta.class",
          "meta.function",
          "meta.function-call",
          "meta.preprocessor",
          "meta.return-type",
          "meta.selector",
          "meta.tag",
          "meta.type.annotation",
          "meta.type",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end",
          "punctuation.separator",
          "punctuation.separator.continuation",
          "punctuation.terminator",
          "storage",
          "storage.modifier",
          "storage.type",
          "string",
          "string.interpolated",
          "string.other",
          "string.quoted",
          "string.quoted.double",
          "string.quoted.other",
          "string.quoted.single",
          "string.quoted.triple",
          "string.regexp",
          "string.unquoted",
          "strong",
          "support",
          "support.class",
          "support.constant",
          "support.function",
          "support.other",
          "support.type",
          "support.type.property-name",
          "support.variable",
          "variable",
          "variable.language",
          "variable.name",
          "variable.other",
          "variable.other.readwrite",
          "variable.parameter",
          "storage.type.ts",
          "keyword.control.flow.ts",
          "meta.property"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ],
    "[Cursor Dark Anysphere]": {
      "strings": "#977CDC",
      "numbers": "#977CDC",
      "textMateRules": [
        {
          "scope": [
            "entity.other.attribute-name.tsx",
            "entity.other.attribute-name.jsx"
          ],
          "settings": {
            "foreground": "#977CDC",
            "fontStyle": ""
          }
        },
        {
          "scope": ["keyword", "entity.name.function"],
          "settings": {
            "foreground": "#977CDC"
          }
        },
        {
          "scope": [
            "entity.other.attribute-name.jsx",
            "variable.other.readwrite.jsx",
            "variable.other.readwrite.tsx"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        }
      ]
    },
    "[CodeSandbox]": {
      "strings": "#977CDC",
      "numbers": "#977CDC",
      "textMateRules": [
        {
          "scope": "comment.markup-link",
          "settings": {
            "foreground": "#DFE2E7"
          }
        },
        {
          "scope": "keyword.operator.new.js",
          "settings": {
            "foreground": "#977CDC"
          }
        },
        {
          "scope": "support.type.property-name",
          "settings": {
            "foreground": "#77B7D7"
          }
        },
        {
          "scope": [
            "support.variable.property.js",
            "support.variable.property.ts",
            "support.variable.property.tsx"
          ],
          "settings": {
            "foreground": "#FBCC43"
          }
        },
        {
          "scope": [
            "variable.other.constant.property.js",
            "variable.other.constant.property.ts",
            "variable.other.constant.property.tsx"
          ],
          "settings": {
            "foreground": "#FBCC43"
          }
        }
      ]
    }
  },
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.lineHeight": 20,
  "editor.stickyScroll.enabled": true,
  "git.ignoreMissingGitWarning": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "editor.fontWeight": "500",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "window.zoomLevel": 1,
  "explorer.confirmDragAndDrop": false,
  "git.openRepositoryInParentFolders": "never",
  "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "lldb.launch.expressions": "native",
  "lldb.suppressUpdateNotifications": true
}
